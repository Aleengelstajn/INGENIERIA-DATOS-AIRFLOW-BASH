   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import plotly.graph_objects as go\n",
    "from pandarallel import pandarallel"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "d = pd.read_csv('datasets/taxi_api.csv')\n",
    "df = pd.DataFrame(pd.to_datetime(d.tpep_pickup_datetime))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getDay(param):\n",
    "    return str(param).split(':')[0]\n",
    "\n",
    "def getHour(param):\n",
    "    return int(param.strftime(\"%H\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "INFO: Pandarallel will run on 4 workers.\n",
      "INFO: Pandarallel will use standard multiprocessing data transfer (pipe) to transfer data between the main process and workers.\n",
      "\n",
      "WARNING: You are on Windows. If you detect any issue with pandarallel, be sure you checked out the Troubleshooting page:\n",
      "https://nalepae.github.io/pandarallel/troubleshooting/\n"
     ]
    }
   ],
   "source": [
    "pandarallel.initialize()\n",
    "df['dia_hora'] = df.tpep_pickup_datetime.parallel_apply(getDay)\n",
    "df['hora'] = df.tpep_pickup_datetime.parallel_apply(getHour)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "# mask dia\n",
    "#df['dia_hora'] = mask_dia\n",
    "#df.dia_hora = df.dia_hora.astype('str')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "#mask_hora\n",
    "#df['hora'] = mask_hora\n",
    "#df['dia'] = mask_dia\n",
    "#df.hora = df.hora.astype('int')\n",
    "\n",
    "#horas_dia\n",
    "#df.dia_hora = df.dia_hora + '-' + df.hora.astype('str')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "d = df.groupby(['dia_hora','hora']).size()\n",
    "statistics = pd.DataFrame(d.values, columns=['viajes_amount'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "# get index from statistics\n",
    "horas = np.zeros((d.values.shape[0],1))\n",
    "for idx, i in enumerate(d.index):\n",
    "    horas[idx] = i[1]\n",
    "\n",
    "statistics['hora'] = horas"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "days = statistics.groupby('hora').median().index\n",
    "y0 = statistics.groupby('hora').median().viajes_amount"
   ]
